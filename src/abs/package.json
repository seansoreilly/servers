{
  "name": "@modelcontextprotocol/server-abs",
  "version": "0.1.0",
  "description": "MCP Server for the Australian Bureau of Statistics (ABS) Data API",
  "license": "MIT",
  "type": "module",
  "bin": {
    "mcp-server-abs": "index.js"
  },
  "scripts": {
    "build": "tsc && shx chmod +x *.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "test": "node --loader ts-node/esm test.ts",
    "start": "npm run build && node index.js",
    "dev": "nodemon *.ts",
    "debug": "node --inspect --loader ts-node/esm index.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.0.4",
    "express": "^4.21.2",
    "node": "^23.5.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "nodemon": "^3.1.9",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
